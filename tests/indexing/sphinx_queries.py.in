# output format: query\n

d = {}
topicid = None

for line in open("${CMAKE_CURRENT_BINARY_DIR}/source/trec9-train/query.ohsu.1-63", "r"):
  if line.startswith("<num>"):
    topicid = line.split()[-1].strip()
  if line.startswith("<title>"):
    query = line[7:].strip()
    d[query] = topicid

out = open("${CMAKE_CURRENT_BINARY_DIR}/queries.txt", "w")

for key in d:
  out.write(key + "\n")

out.close()

